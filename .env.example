# Frontend Environment Variables
# Copy this file to .env.local and update the values as needed

# API Base URL - use this if you want to override the default localhost:8080
# Leave commented out for default local development
# VITE_API_URL=http://localhost:8080

# Port for Vite dev server (default: 5000)
VITE_PORT=5000

# Enable/Disable Vite HMR (Hot Module Replacement)
VITE_HMR_ENABLED=true
